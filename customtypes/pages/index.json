            "hero": { "type": "SharedSlice" },
            "company_list": { "type": "SharedSlice" },
            "text_with_slider": { "type": "SharedSlice" },
            "text_with_icons": { "type": "SharedSlice" },
            "time_line": { "type": "SharedSlice" },
            "portfolio": { "type": "SharedSlice" }
          }
        }
      }
